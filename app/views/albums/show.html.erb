<h1><%= @album.name %></h1>
<%= link_to "Edit Album", edit_album_path %>
<%= link_to "Delete Album", album_path, :method => :delete, :data => { :confirm => 'Are you sure?' } %>
<%= link_to "Add a photo", new_photo_path(:photo => {album_id: @album.id}) %>
<% @album.photos.each do |photo| %>
  <%= render photo %>
  <%= photo.name %>
  <%= photo.created_at %>
  <%= link_to "Edit photo", edit_photo_path %>
  <%= link_to "Delete photo", photo_path, :method => :delete,  :data => { :confirm => 'Are you sure you want to delete this?' } %>
<% end %>  
